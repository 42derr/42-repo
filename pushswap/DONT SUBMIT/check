#!/bin/bash

# Generate 100 random different numbers between 1 and 1000
ARG=$(shuf -i 1-1000 -n 3 | uniq)

# Count the unique numbers
count=$(echo "$ARG" | wc -w )

# Output the count
echo "Total number of unique random numbers: $count"

# Run push_swap, capture the output, and count the lines
push_swap_output=$(./push_swap $ARG)
push_swap_lines=$(echo "$push_swap_output" | wc -l )
echo "Number of lines generated by push_swap: $push_swap_lines"

# Run checker_linux with the generated numbers
echo "$push_swap_output" | ./checker_linux $ARG

# ARG="5 4 3 2 1"; ./push_swap $ARG | ./checker_linux $ARG

#ARG="10 7 8 2 3 6 1 4 9 5"; ./push_swap $ARG | ./checker $ARG