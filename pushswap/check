#!/bin/bash

# Generate 100 random different numbers between 1 and 1000
ARG=$(shuf -i 1-1000 -n 100 | uniq)

# Count the unique numbers
count=$(echo "$ARG" | wc -w)

# Output the count
echo "Total number of unique random numbers: $count"

# Run push_swap, capture the output, and count the lines
push_swap_output=$(./push_swap $ARG)
push_swap_lines=$(echo "$push_swap_output" | wc -l )
echo "Number of lines generated by push_swap: $push_swap_lines"

# Run checker_linux with the generated numbers
echo "$push_swap_output" | ./checker_linux $ARG

# ARG="367 24 32 428 827 256 14 602 170 642 945 572 60 386 702 957 245 850 369 466 161 601 9 304 515 143 52 254 454 451 677 701 670 518 663 558 678 577 5 560 531 459 166 192 774 436 833 739 743 59 789 634 908 334 227 705 190 742 651 706 548 206 829 480 399 725 106 530 746 594 383 265 685 489 156 381 250 95 448 198 852 952 669 211 20 116 798 117 464 995 562 214 839 990 475 141 233 502 782"; ./push_swap $ARG | ./checker_linux $ARG
